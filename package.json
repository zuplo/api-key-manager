{
  "name": "@zuplo/api-key-manager",
  "version": "0.17.0",
  "description": "A React component to manage API keys",
  "license": "MIT",
  "author": "Zuplo",
  "repository": {
    "type": "git",
    "url": "https://github.com/zuplo/react-api-key-manager"
  },
  "scripts": {
    "build": "npm run build --workspace packages/react",
    "dev": "npm run dev --workspace examples/nextjs",
    "format": "prettier --write .",
    "lint": "eslint --ext .ts,.tsx .",
    "release": "npm version minor",
    "version": "npm version $npm_package_version --workspaces && git add **/package.json",
    "postversion": "git push --tags && git push"
  },
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "devDependencies": {
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "typescript": "^5.0.2"
  }
}
